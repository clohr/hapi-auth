{
    "server": {
        "app": {
            "description": "hapi-yar"
        }
    },
    "connections": [
        {
            "port": 9000,
            "labels": ["web-ui", "api"]
        }
    ],
    "plugins": {
        "./api": null,
        "./cookie": null,
        "good": {
            "reporters": [{
                "reporter": "good-console",
                "args": [{
                    "log": "*",
                    "response": "*"
                }]
            }]
        },
        "./home": null,
        "./login": null,
        "./reverse": null,
        "./static": null,
        "./token": null,
        "visionary": {
            "engines": {
                "hbs": "handlebars"
            },
            "path": "./views",
            "layoutPath": "./views/layout",
            "partialsPath": "./views/partials",
            "layout": true
        }
    }
}